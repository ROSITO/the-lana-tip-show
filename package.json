{
  "name": "the-lana-tip-show",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "export POSTGRES_URL=${POSTGRES_URL:-${lana_POSTGRES_URL:-${PRISMA_DATABASE_URL:-${lana_PRISMA_DATABASE_URL:-${DATABASE_URL:-${lana_DATABASE_URL}}}}}} && prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:init": "tsx scripts/init-db.ts"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.424.0",
    "@prisma/client": "^5.9.1",
    "prisma": "^5.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "tsx": "^4.7.1"
  }
}

